{
  "productName": "Pamidor",
  "name": "pamidor",
  "version": "1.0.0",
  "description": "A minimal pomodoro timer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "mac-build": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --app-bundle-id=com.pamidor.dvdqapps --prune=true --out=release-builds",
    "win-build": "electron-packager . pamidor --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Pamidor\" && node ./build/win-installer.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "David Qui√±ones",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.3",
    "electron-packager": "^14.0.4",
    "electron-winstaller": "^4.0.0"
  }
}
